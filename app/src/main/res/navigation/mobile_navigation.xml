<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.worldskillsshop.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_navigation_home_to_addingAds"
            app:destination="@id/addingAds" />
    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.worldskillsshop.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_navigation_dashboard_to_reviews_frag"
            app:destination="@id/reviews_frag" />
    </fragment>

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.worldskillsshop.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" >
        <action
            android:id="@+id/action_navigation_notifications_to_favorites_frag"
            app:destination="@id/favorites_frag" />
        <action
            android:id="@+id/action_navigation_notifications_to_settings_frag"
            app:destination="@id/settings_frag" />
        <action
            android:id="@+id/action_navigation_notifications_to_orders_frag"
            app:destination="@id/orders_frag" />
        <action
            android:id="@+id/action_navigation_notifications_to_reviews_frag"
            app:destination="@id/reviews_frag" />
        <action
            android:id="@+id/action_navigation_notifications_to_coupons_frag"
            app:destination="@id/coupons_frag" />
        <action
            android:id="@+id/action_navigation_notifications_to_ads_frag"
            app:destination="@id/ads_frag" />
        <action
            android:id="@+id/action_navigation_notifications_to_posts_frag"
            app:destination="@id/posts_frag" />
    </fragment>
    <fragment
        android:id="@+id/settings_frag"
        android:name="com.example.worldskillsshop.Settings_frag"
        android:label="fragment_settings_frag"
        tools:layout="@layout/fragment_settings_frag" />
    <fragment
        android:id="@+id/ads_frag"
        android:name="com.example.worldskillsshop.ads_frag"
        android:label="fragment_ads_frag"
        tools:layout="@layout/fragment_ads_frag" >
        <action
            android:id="@+id/action_ads_frag_to_addingAds"
            app:destination="@id/addingAds" />
    </fragment>
    <fragment
        android:id="@+id/coupons_frag"
        android:name="com.example.worldskillsshop.Coupons_frag"
        android:label="fragment_coupons_frag"
        tools:layout="@layout/fragment_coupons_frag" />
    <fragment
        android:id="@+id/orders_frag"
        android:name="com.example.worldskillsshop.Orders_frag"
        android:label="fragment_orders_frag"
        tools:layout="@layout/fragment_orders_frag" />
    <fragment
        android:id="@+id/posts_frag"
        android:name="com.example.worldskillsshop.Posts_frag"
        android:label="fragment_posts_frag"
        tools:layout="@layout/fragment_posts_frag" >
        <action
            android:id="@+id/action_posts_frag_to_messageView"
            app:destination="@id/messageView" />
    </fragment>
    <fragment
        android:id="@+id/reviews_frag"
        android:name="com.example.worldskillsshop.Reviews_frag"
        android:label="fragment_reviews_frag"
        tools:layout="@layout/fragment_reviews_frag" />
    <fragment
        android:id="@+id/favorites_frag"
        android:name="com.example.worldskillsshop.Favorites_frag"
        android:label="fragment_favorites_frag"
        tools:layout="@layout/fragment_favorites_frag" />
    <fragment
        android:id="@+id/addingAds"
        android:name="com.example.worldskillsshop.AddingAds"
        android:label="fragment_adding_ads"
        tools:layout="@layout/fragment_adding_ads" />
    <fragment
        android:id="@+id/messageView"
        android:name="com.example.worldskillsshop.MessageView"
        android:label="fragment_message_view"
        tools:layout="@layout/fragment_message_view" />
    <activity
        android:id="@+id/openAdsA"
        android:name="com.example.worldskillsshop.OpenAdsA"
        android:label="activity_open_ads"
        tools:layout="@layout/activity_open_ads" />
    <activity
        android:id="@+id/addingAdsA"
        android:name="com.example.worldskillsshop.addingAdsA"
        android:label="activity_adding_ads"
        tools:layout="@layout/activity_adding_ads" />
    <activity
        android:id="@+id/dialogueOpen"
        android:name="com.example.worldskillsshop.DialogueOpen"
        android:label="activity_dialogue_open"
        tools:layout="@layout/activity_dialogue_open" />
    <activity
        android:id="@+id/progress"
        android:name="com.example.worldskillsshop.progress"
        android:label="activity_progress"
        tools:layout="@layout/activity_progress" />
    <fragment
        android:id="@+id/description2"
        android:name="com.example.worldskillsshop.description"
        android:label="fragment_description"
        tools:layout="@layout/fragment_description" />
    <fragment
        android:id="@+id/openAds"
        android:name="com.example.worldskillsshop.OpenAds"
        android:label="fragment_open_ads"
        tools:layout="@layout/fragment_open_ads" />
</navigation>